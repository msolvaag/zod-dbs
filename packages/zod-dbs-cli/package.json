{
  "name": "zod-dbs-cli",
  "version": "1.0.3",
  "description": "Opinionated Zod schema generator for databases",
  "main": "./dist/index.js",
  "type": "module",
  "bin": {
    "zod-dbs": "index.js"
  },
  "scripts": {
    "build": "rm -rf ./dist && tsc --project tsconfig.build.json",
    "test:unit": "vitest --project unit run",
    "test:integration": "pnpm run test:integration:cli && pnpm run test:integration:schema",
    "test:integration:cli": "vitest --project integration:cli run",
    "test:integration:schema": "vitest --project integration:schema run",
    "test:types": "tsc --noEmit"
  },
  "author": "Magnus Solv√•g",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/msolvaag/zod-dbs.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist"
  ],
  "devDependencies": {
    "@testcontainers/mysql": "^11.5.1"
  },
  "dependencies": {
    "chalk": "^5.4.1",
    "commander": "^14.0.0",
    "cosmiconfig": "^9.0.0",
    "mustache": "^4.2.0",
    "ora": "^8.2.0",
    "zod-dbs": "workspace:*",
    "zod-dbs-core": "workspace:*",
    "zod-dbs-pg": "workspace:*",
    "zod-dbs-mysql": "workspace:*"
  }
}
